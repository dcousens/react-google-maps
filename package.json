{
  "name": "react-maps",
  "version": "0.3.1",
  "description": "",
  "author": "Daniel Cousens",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcousens/react-maps.git"
  },
  "bugs": {
    "url": "https://github.com/dcousens/react-maps/issues"
  },
  "homepage": "https://github.com/dcousens/react-maps",
  "keywords": [
    "react-maps"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "babel --out-dir lib/ src/",
    "build-examples": "mkdir -p examples/build && browserify -t babelify examples/src/example1.js --outfile examples/build/example1.js",
    "coverage": "mocha --require blanket -R travis-cov",
    "coverage-local": "mocha --require blanket -R html-cov",
    "prepublish": "npm run build",
    "standard": "standard",
    "test": "npm run standard && npm run unit",
    "unit": "mocha --compilers js:babel/register",
    "watch": "babel --watch --out-dir lib/ src/",
    "watch-examples": "mkdir -p examples/build && watchify -t babelify examples/src/example1.js --outfile examples/build/example1.js"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    },
    "travis-cov": {
      "threshold": 100
    }
  },
  "standard": {
    "ignore": [
      "lib/"
    ]
  },
  "dependencies": {
    "blacklist": "^1.1.2",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel": "*",
    "babelify": "*",
    "blanket": "*",
    "browserify": "*",
    "mocha": "*",
    "standard": "*",
    "travis-cov": "*",
    "watchify": "*"
  }
}
